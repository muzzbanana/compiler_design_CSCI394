let
    var max := 35
    function numberize(i: int) =
        if (i / 15) * 15 = i then print("fizzbuzz\n")
        else if (i / 3) * 3 = i then print("fizz\n")
        else if (i / 5) * 5 = i then print("buzz\n")
        else (print_int(i); print("\n"))
in
    for num := 1 to max do numberize(num)
end
