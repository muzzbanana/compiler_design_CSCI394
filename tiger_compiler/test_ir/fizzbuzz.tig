let
    var max := 35
    function multiple(num: int, divisor: int) = ((num / divisor) * divisor = num)
    function numberize(i: int) =
        if multiple(i, 15) then print("fizzbuzz\n")
        else if multiple(i, 3) then print("fizz\n")
        else if multiple(i, 5) then print("buzz\n")
        else (print_int(i); print("\n"))
in
    for num := 1 to max do numberize(num)
end
